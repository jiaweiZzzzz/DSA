// 模拟加法运算
vector<int> add(vector<int> a, vector<int> b){
  if(a.size() < b.size()) return add(b, a);
  
  vector<int> res;
  // 模拟
  for(int i = 0, int t = 0; i < a.size(); i++){
      t += a[i];
      if(i < b.size()) t += b[i];
      res.push_back(t % 10);
      t /= 10;
  }
  if(t) res.push_back(1);
  
  return res;
}
