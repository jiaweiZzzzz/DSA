#include <bits/stdc++.h>
using namespace std;

const int N = 100010;
int n, m;
int a[N], s[N];

int main(){
	cin >> n >> m;
	for(int i = 1; i <= n; i++) cin >> a[i];
	
  // 初始化前缀和
	for(int i = 1; i <= n; i++) s[i] = s[i - 1] + a[i];
	
	while(m--)
	{
    // 求区间和
		int x, y;
		cin >> x >> y;
		cout << s[y] - s[x - 1] << endl;	
	} 
	
	return 0;
} 
